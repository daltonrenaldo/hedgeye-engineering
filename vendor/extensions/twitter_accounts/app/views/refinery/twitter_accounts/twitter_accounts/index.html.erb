<% content_for :body do %>
  <ul id="twitter_accounts">
    <% @twitter_accounts.each do |twitter_account| %>
      <li>
        <%= link_to twitter_account.username, refinery.twitter_accounts_twitter_account_path(twitter_account) %>
      </li>
    <% end %>
  </ul>
<% end %>

<%= render '/refinery/content_page' %>
